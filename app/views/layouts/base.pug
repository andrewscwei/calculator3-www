extends ../../../node_modules/pug-base-layout/index

append config
  -
    $appName = $config.title
    $title = $config.title
    $description = $config.description
    $keywords = $config.keywords
    $url = $config.url
    $analytics = $config.analytics
    $manifestPath = path => require.context('manifest', true, /.*/)(`./${path}`)

block styles
  link(rel=`stylesheet` href=`https://fonts.googleapis.com/css?family=Roboto+Slab:100,300,400|Roboto:100,100i,300,300i,500,500i`)
